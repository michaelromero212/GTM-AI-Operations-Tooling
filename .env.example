# https://app.relevanceai.com
RELEVANCE_PROJECT_ID=your_project_id_here
RELEVANCE_API_KEY=your_api_key_here
RELEVANCE_AGENT_ID=your_agent_id_here
RELEVANCE_REGION=your_region_here
